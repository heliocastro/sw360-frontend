	"devDependencies": {
		"@sveltejs/adapter-auto": "^1.0.0-next.33",
		"@sveltejs/kit": "^1.0.0-next.297",
		"autoprefixer": "^10.4.2",
		"eslint": "^7.32.0",
		"eslint-config-prettier": "^8.3.0",
		"eslint-plugin-svelte3": "^3.4.0",
		"marked": "^4.0.12",
		"postcss": "^8.4.6",
		"prettier": "^2.4.1",
		"prettier-plugin-svelte": "^2.4.0",
		"request": "^2.88.2",
		"sass": "^1.49.9",
		"svelte": "^3.44.0",
		"svelte-check": "^2.4.6",
		"svelte-fa": "^2.4.0",
